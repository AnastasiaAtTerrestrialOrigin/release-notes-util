{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ESNext",
        ],
        "skipLibCheck": true,
        "moduleResolution": "node",
        "noEmit": false,
        "strict": true,
        "outDir": "dist-electron",
        "module": "CommonJS",
        "esModuleInterop": true
    },
    "include": [
        "src/electronMain.ts",
        "src/preload.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}